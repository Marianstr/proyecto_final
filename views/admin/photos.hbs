         <div class="col-7">
                <span> Hola {{usuario}} </span>
         </div>
         <div class="col-2" style="float:right;">
            Volver a <a href="/">Home</a>
         </div>
        <div class="col-3" >
            <a style="float:right;" href="/admin/login/logout" class="btn btn-danger">Cerrar sesi√≥n<i class="fas fa-sign-out-alt"></i></a>                      
        </div> 
        
